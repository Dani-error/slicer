{
    "arrowParens": "always",
    "singleQuote": false,
    "semi": true,
    "trailingComma": "es5",
    "organizeImportsSkipDestructiveCodeActions": true,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
